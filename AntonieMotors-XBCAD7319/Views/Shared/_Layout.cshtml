<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AntonieMotors_XBCAD7319</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/AntonieMotors_XBCAD7319.styles.css" asp-append-version="true" />
</head>
<body>
    <div id="page-wrapper">
        <header id="header">
            <h1 id="logo"><a href="~/Home/Index">Antonie Motors</a></h1>
            <nav id="nav">
                <ul>
                    <li><a asp-controller="Home" asp-action="Index">Home</a></li>
                    <li>
                        <a asp-controller="Login" asp-action="UserLogin">Login</a>
                       
                    </li>
                    <li><a asp-controller="Customer" asp-action="Register" class="button primary">Register</a></li>
                </ul>
            </nav>
        </header>

        <div class="container">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>

      @*   <footer class="border-top footer text-muted">
            <div class="container">
                &copy; 2024 - AntonieMotors_XBCAD7319 - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
            </div>
        </footer> *@
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
